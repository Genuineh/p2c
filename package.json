{
  "name": "pixso-codeforge",
  "version": "0.1.0",
  "description": "Pixso CodeForge - 国内最强多端代码生成插件，支持 Flutter / SwiftUI / Jetpack Compose / React / Vue 3 / 微信小程序",
  "type": "module",
  "scripts": {
    "dev": "vite build --watch",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,html,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,html,css}\"",
    "prepare": "husky",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "pixso",
    "plugin",
    "code-generation",
    "flutter",
    "swiftui",
    "jetpack-compose",
    "react",
    "vue3",
    "weapp",
    "design-to-code"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^19.0.0",
    "@commitlint/config-conventional": "^19.0.0",
    "@figma/plugin-typings": "^1.121.0",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.0",
    "husky": "^9.0.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.0",
    "typescript": "^5.3.0",
    "vite": "^5.1.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,html,css}": [
      "prettier --write"
    ]
  }
}
